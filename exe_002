nome = input('Qual é seu nome?').strip()
print('É um prazer te conhecer {}!'.format(nome))

nome = input("Qual é seu nome?")
print('É um prazer te conhecer, {}!'.format(nome))

nome = input('Qual é seu nome?')
print('Tudo bem, {}?'.format(nome))

nome = input('Digite seu nome:')
print(f'É um prazer te conhecer {nome}')
